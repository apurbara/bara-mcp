<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="Shared\Domain\Model\Form\SelectField" 
            table="SelectField" 
    >
        <id name="id" type="guid" />
        <embedded name="field" 
                  class="Shared\Domain\Model\Form\FieldVO" 
                  use-column-prefix="false" 
        />
        <one-to-many  field="options" 
                      target-entity="Shared\Domain\Model\Form\SelectField\Option"
                      mapped-by="selectField"
        />
    </entity>
</doctrine-mapping>
